group {
   name: "slider_layout";
   parts{
      part { name: "elm.swallow.bg";
         type: SWALLOW;
            scale: 1;
               description { state: "default" 0.0;
                  }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
            scale: 1;
               description { state: "default" 0.0;
                  }
      }
      part { name: "elm.text.slider";
         type: TEXT;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 1;
            rel1.relative: 0.5 0.5;
            rel2.relative: 0.5 0.5;
            text {
               font: "Tizen:style=Regular";
               size: 50;
               min: 1 1;
            }
            color: 255 255 255 255;
         }
      }
   }
}
